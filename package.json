{
	"name": "eslint-config-airbnb-flat",
	"type": "module",
	"version": "0.0.2",
	"description": "Unofficial migration of the airbnb styleguide from eslintrc format to flat config file format",
	"author": "error-four-o-four",
	"license": "MIT",
	"homepage": "https://github.com/error-four-o-four/eslint-config-airbnb-flat",
	"repository": {
		"type": "git",
		"url": "https://github.com/error-four-o-four/eslint-config-airbnb-flat.git"
	},
	"bugs": {
		"url": "https://github.com/error-four-o-four/eslint-config-airbnb-flat/issues"
	},
	"keywords": [
		"eslint",
		"eslint-config",
		"eslint-flat-config",
		"airbnb",
		"styleguide",
		"style-guide",
		"unofficial"
	],
	"files": [
		"dist",
		"README.md"
	],
	"exports": {
		".": {
			"types": "./dist/configs.d.ts",
			"import": "./dist/configs/index.js"
		},
		"./base": {
			"types": "./dist/base/index.d.ts",
			"import": "./dist/base/index.js"
		},
		"./compat": {
			"types": "./dist/compat/index.d.ts",
			"import": "./dist/compat/index.js"
		}
	},
	"engines": {
		"node": ">=18"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"lint:check": "eslint .",
		"lint:fix": "eslint . --fix",
		"cli:extract": "node --no-warnings=ExperimentalWarning --loader ts-node/esm scripts/extractConfigKeys.ts",
		"cli:generate": "node --no-warnings=ExperimentalWarning --loader ts-node/esm scripts/generateConfigs.ts",
		"cli:merge": "node --no-warnings=ExperimentalWarning --loader ts-node/esm scripts/mergeConfigs.ts",
		"clean:base": "rimraf dist/base",
		"build:base": "tsup-node --entry.index src/defineBaseConfig.ts --format esm --minify --dts -d ./dist/base",
		"clean:compat": "rimraf dist/compat",
		"build:compat": "tsup-node --entry.index src/compat.ts --format esm --minify --dts -d ./dist/compat",
		"clean:configs": "rimraf dist/configs",
		"build:configs": "tsup-node --entry.index src/configs.ts --format esm --minify --dts -d ./dist/configs",
		"clean": "rimraf dist/",
		"build": "npm run clean && npm run build:all",
		"build:all": "npm run build:base && npm run build:compat && npm run build:configs",
		"depr:build:base": "esbuild src/defineBaseConfig.js --bundle --minify --platform=node --target=node18 --format=esm --packages=external --outfile=dist/defineBaseConfig.min.js",
		"depr:build:base:dts": "tsup-node --entry.defineBaseConfig src/defineBaseConfig.js --format esm,cjs --dts -d ./dist"
	},
	"dependencies": {
		"@stylistic/eslint-plugin": "^1.5.1",
		"@typescript-eslint/eslint-plugin": "^6.16.0",
		"@typescript-eslint/parser": "^6.16.0",
		"eslint": "^8.56.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-i": "^2.29.1",
		"eslint-plugin-n": "^16.5.0",
		"globals": "^13.24.0",
		"local-pkg": "^0.5.0"
	},
	"devDependencies": {
		"@antfu/eslint-define-config": "^1.23.0-2",
		"@eslint/eslintrc": "^2.1.4",
		"@tsconfig/node18": "^18.2.2",
		"@types/eslint": "^8.56.0",
		"@types/eslint__eslintrc": "^2.1.1",
		"@types/node": "^20.10.5",
		"esbuild": "^0.19.10",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-flat-config-viewer": "^0.1.3",
		"rimraf": "^5.0.5",
		"ts-node": "^10.9.2",
		"tslib": "^2.6.2",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3"
	}
}
